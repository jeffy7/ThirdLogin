{
    "title": "分享视频消息",
    "grouped": true,
    "controllerName": "QQAPIDemoCommonController",
    "sections":
    [
        {
            "elements":
            [
                {
                    "type": "QTextElement",
                    "text": "分享视频消息"
                }
            ]
        },
        {
            "elements":
            [
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "title",
                    "placeholder": "填入分享的标题",
                    "textValue": "定向分享标题",
                    "bind": "textValue:binding_title"
                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "description",
                    "placeholder": "填入分享的描述",
                    "textValue": "定向分享描述",
                    "bind": "textValue:binding_description"
                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "url",
                    "placeholder": "填入分享的URL链接",
                    "textValue": "http://www.163.com",
                    "bind": "textValue:binding_url"
                },
                {
                    "type": "QImageElement",
                    "title": "preview image",
                    "placeholder": "点击选择分享的图片",
                    "source": "photoLibrary",
                    "bind": "imageValue:binding_previewImage"
                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "stream url",
                    "placeholder": "填入分享音视频的流媒体URL链接",
                    "bind": "textValue:binding_streamUrl"
                }
            ]
        },
        {
            "elements":
            [
                {
                    "type": "QButtonElement",
                    "key": "shareBtn",
                    "title": "分享",
                    "controllerAction": "onShareVideo:"
                }
            ]
        }
    ]
}
