{
    "title": "分享新闻(网络图片)消息",
    "grouped": true,
    "controllerName": "QQAPIDemoCommonController",
    "sections":
    [
        {
            "elements":
            [
                {
                    "type": "QTextElement",
                    "text": "分享新闻(网络图片)消息"
                }
            ]
        },
        {
            "elements":
            [
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "title",
                    "placeholder": "填入分享的标题",
                    "textValue": "定向分享标题",
                    "bind": "textValue:binding_title"
                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "description",
                    "placeholder": "填入分享的描述",
                    "textValue": "定向分享描述",
                    "bind": "textValue:binding_description"
                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "url",
                    "placeholder": "填入分享的URL链接",
                    "textValue": "http://www.163.com",
                    "bind": "textValue:binding_url"
                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "preview image url",
                    "placeholder": "填入分享预览图的URL链接",
                    "textValue": "http://cdni.wired.co.uk/620x413/k_n/NewsForecast%20copy_620x413.jpg",
                    "bind": "textValue:binding_previewImageUrl"
                }
            ]
        },
        {
            "elements":
            [
                {
                    "type": "QButtonElement",
                    "key": "shareBtn",
                    "title": "分享",
                    "controllerAction": "onShareNewsWeb:"
                }
            ]
        }
    ]
}
